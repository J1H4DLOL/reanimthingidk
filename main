	if particle1 then
		particle1.Size = NumberSequence.new({NumberSequenceKeypoint.new(0,500), NumberSequenceKeypoint.new(1,0)})
	end
	if particle2 then
		particle2.Size = NumberSequence.new({NumberSequenceKeypoint.new(0,500), NumberSequenceKeypoint.new(1,0)})
	end
	for i = 1,20 do
